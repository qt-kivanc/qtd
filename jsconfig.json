{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "allowJs": false,
    "declaration": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es6",
      "es2017"
    ],
    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "jsx": "react",
    "strict": false,
    "baseUrl": "./",
    "paths": {
      "app/*": ["src/*"],
      "context/*": ["src/context/*"],
      "./*": ["./node_modules/"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "rollup.config.js"]
}
